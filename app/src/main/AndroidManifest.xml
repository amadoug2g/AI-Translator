<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.playgroundagc.deepltranslator">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_deep_l_logo_foreground"
        android:label="@string/app_name_2"
        android:roundIcon="@mipmap/ic_deep_l_logo_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.DeepLTranslator">
        <activity android:name=".app.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>